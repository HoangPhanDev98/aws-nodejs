  "license": "MIT",
  "scripts": {
    "scss": "sass scss/main.scss public/assets/css/bootstrap/main.css",
    "scss:watch": "sass --watch scss/main.scss public/assets/css/bootstrap/main.css",
    "build": "npm run scss && tsc",
    "start": "node ./dist/server.js",
    "dev": "npm run build && node ./dist/server.js",
    "dev:watch": "nodemon ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "prettier --check \"src/**/*.ts\" && eslint \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.317.0",
    "@aws-sdk/s3-request-presigner": "^3.317.0",
    "@types/axios": "^0.14.0",
    "@types/big.js": "^6.1.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/lodash": "^4.14.191",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "aws-sdk": "^2.1361.0",
    "axios": "^1.3.3",
    "big.js": "^6.2.1",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-validator": "^6.14.3",
    "http-status": "^1.6.2",
    "jquery": "^3.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.58.1",
    "typescript": "^5.0.4"
